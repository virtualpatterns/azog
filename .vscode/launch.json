{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Sandbox Convert w/ Short Video",
      "program": "${workspaceFolder}/distributable/sandbox/convert.js",
      "preLaunchTask": "Build",
      "args": [
        "--configurationPath", "${workspaceFolder}/distributable/sandbox/convert-configuration.json",
        "${workspaceFolder}/resource/deluge/downloaded/Jonathan.2018.1080p.WEB-DL.DD5.1.H264-FGT/Jonathan.2018.1080p.WEB-DL.DD5.1.H264-FGT.mp4"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Sandbox Probe w/ Short Video",
      "program": "${workspaceFolder}/distributable/sandbox/probe.js",
      "preLaunchTask": "Build",
      "args": [
        "${workspaceFolder}/resource/deluge/downloaded/Jonathan.2018.1080p.WEB-DL.DD5.1.H264-FGT/Jonathan.2018.1080p.WEB-DL.DD5.1.H264-FGT.mp4"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Test",
      "program": "${workspaceFolder}/node_modules/.bin/_mocha",
      "preLaunchTask": "Build",
      "args": [
        "--bail", 
        "--timeout", "0",
        "${workspaceFolder}/distributable/test/index.js"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch w/ Invalid",
      "program": "${workspaceFolder}/distributable/command/index.js",
      "preLaunchTask": "Build",
      "args": [
        "--configurationPath", "${workspaceFolder}/resource/deluge/configuration.json",
        "0", "0", "0"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch w/ Album",
      "program": "${workspaceFolder}/distributable/command/index.js",
      "preLaunchTask": "Build",
      "args": [
        "--configurationPath", "${workspaceFolder}/resource/deluge/configuration.json",
        "--logLevel", "trace", "--logPath", "console",
        "0001", "Jimi Hendrix - Axis Bold as Love (320)", "${workspaceFolder}/resource/deluge/downloaded"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch w/ Short Video",
      "program": "${workspaceFolder}/distributable/command/index.js",
      "preLaunchTask": "Build",
      "args": [
        "--configurationPath", "${workspaceFolder}/resource/deluge/configuration.json",
        "0001", "Jonathan.2018.1080p.WEB-DL.DD5.1.H264-FGT", "${workspaceFolder}/resource/deluge/downloaded"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}