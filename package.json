  "description": "TBD",
  "files": [
    "dist/cli",
    "dist/config.js",
    "dist/config.js.map"
  ],
  "license": "GPL-3.0+",
  "name": "@virtualpatterns/azog",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/azog"
  },
  "scripts": {
    "refresh": "rm -Rf node_modules dist/* && npm install",
    "clean": "rm -Rf dist/config.js dist/config.js.map dist/tasks",
    "lint": "npm run clean && eslint --ignore-path .gitignore src/tasks",
    "build": "npm run lint && babel src/config.js --out-file dist/config.js --quiet --source-maps && babel src/tasks --copy-files --out-dir dist/tasks --quiet --source-maps",
    "tasks": "npm run build && jake --jakefile dist/tasks/index.js --tasks",
    "jake": "npm run build && jake --jakefile dist/tasks/index.js",
    "process": "npm run jake build && ./dist/cli/bin/on-completed.sh 0",
    "upgrade": "npm-check-updates --upgrade"
  },
  "version": "0.0.2-260",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^5.6.1",
    "jake": "^8.0.15",
    "npm-check-updates": "^2.14.1"
  },
  "dependencies": {
    "@pwn/is": "^1.0.2",
    "@virtualpatterns/mablung": "0.0.22",
    "commander": "^2.19.0",
    "fluent-ffmpeg": "^2.1.2",
    "music-metadata": "^3.1.4",
    "node-tvdb": "^4.1.0",
    "promise-queue": "^2.2.5",
    "sanitize-filename": "^1.6.1",
    "source-map-support": "^0.5.9"
  }
}
